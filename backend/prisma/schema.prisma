generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int @id @default(autoincrement())
  email         String @unique
  name          String
  create_dt     DateTime
  update_dt     DateTime
  events        Event[]
}

model Event {
  id            Int @id @default(autoincrement())
  name          String
  description   String
  start_dt      DateTime
  end_dt        DateTime
  create_dt     DateTime
  update_dt     DateTime
  user          User @relation(fields: [userId], references: [id])
  userId        Int
}